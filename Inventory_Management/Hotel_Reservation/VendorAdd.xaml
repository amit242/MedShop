<Window x:Class="Inventory_Management.VendorAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="VendorAdd" Height="500" Width="350">
    <Grid>
        <Label Content="Phone:" FontSize="8" Height="20" HorizontalAlignment="Left" Margin="16,246,0,0" Name="label18" VerticalAlignment="Top" />
        <TextBox FontSize="8" Height="20" HorizontalAlignment="Left" Margin="85,246,0,0" Name="textBoxVendorPhone" VerticalAlignment="Top" Width="93" />
        <Label Content="E-Mail:" FontSize="8" Height="20" HorizontalAlignment="Left" Margin="16,271,0,0" Name="label19" VerticalAlignment="Top" />
        <TextBox FontSize="8" Height="20" HorizontalAlignment="Left" Margin="85,271,0,0" Name="textBoxVendorEmail" VerticalAlignment="Top" Width="93" />
        <Button Content="Ok" Height="23" HorizontalAlignment="Left" Margin="85,426,0,0" Name="buttonOk" VerticalAlignment="Top" Width="62" Click="buttonOk_Click" />
        <Button Content="Cancle" Height="23" HorizontalAlignment="Left" Margin="164,426,0,0" Name="buttonCancle" VerticalAlignment="Top" Width="78" Click="buttonCancle_Click" />
        <Label Content="First Name:" FontSize="8" Height="20" HorizontalAlignment="Left" Margin="14,10,0,0" Name="label5" VerticalAlignment="Top" Width="54" />
        <TextBox FontSize="8" Height="20" HorizontalAlignment="Left" Margin="85,10,0,0" Name="textBoxVendorFirstName" VerticalAlignment="Top" Width="93" />
        <Label Content="Last Name:" FontSize="8" Height="20" HorizontalAlignment="Left" Margin="14,36,0,0" Name="label8" VerticalAlignment="Top" />
